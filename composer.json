{
    "name": "your-vendor/laravel-yr",
    "description": "A lightweight Laravel package for integrating weather data from Yr (api.met.no)",
    "keywords": ["laravel", "weather", "yr", "met.no"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Your Name",
            "email": "your.email@example.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "YourVendor\\LaravelYr\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "YourVendor\\LaravelYr\\YrServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
